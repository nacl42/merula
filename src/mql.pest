WHITESPACE = _{ " " }

key_char = { ASCII_ALPHANUMERIC | "_" | ":" | "/" | "."  | "-" }
key = @{ key_char+ }

value_char = { !NEWLINE ~ ANY }
value = @{ value_char* }

operator = { "=" | "~" | "<" | ">" }

key_op_value = { key ~ operator ~ value }

mql = {
      SOI ~
      key_op_value | key
      ~ EOI
}

